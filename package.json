{
  "name": "@igloo15/basic-typescript-lib",
  "oldVersion": "0.2.0-dev.1",
  "version": "0.2.0-dev.3",
  "description": "basic typescript lib",
  "main": "dist/index.js",
  "scripts": {
    "update-version": "npm-run-all update-version-pkg update-version-file",
    "update-version-pkg": "node ./config/update-version.js",
    "update-version-file": "node ./config/update-version-file.js",
    "build": "npm run clean && npm run src-build",
    "src-build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "lint": "eslint dist",
    "test": "npm-run-all test:*",
    "test:lint": "npm run lint",
    "test:formatting": "prettier --write src/**/*.{json,ts,tsx,js,jsx,yaml,yml}",
    "fix:formatting": "prettier --check src/**/*.{json,ts,tsx,js,jsx,yaml,yml}",
    "git-tag": "cross-var git tag v$npm_package_version",
    "test-publish": "npm run ready-build && npm pack --dry-run",
    "publish": "npm run ready-build && git-tag && npm publish",
    "ready-build": "npm-run-all clean build test update-version",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igloo15/basic-typescript-lib.git"
  },
  "homepage": "https://github.com/igloo15/basic-typescript-lib#readme",
  "bugs": {
    "url": "https://github.com/igloo15/basic-typescript-lib/issues"
  },
  "author": {
    "name": "igloo15"
  },
  "files": [
    "/dist",
    "package.json"
  ],
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "cross-var": "^1.1.0",
    "eslint": "^6.8.0",
    "git-describe": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "read-pkg": "^5.2.0",
    "replace-in-file": "^5.0.2",
    "rimraf": "^3.0.1",
    "typescript": "^3.7.5",
    "write-pkg": "^4.0.0"
  },
  "readme": "ERROR: No README data found!",
  "_id": "@igloo15/basic-typescript-lib@0.2.0-dev.3",
  "date": "Thu Feb 06 2020"
}
